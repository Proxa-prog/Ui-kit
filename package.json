{
  "name": "vite-ui-kit",
  "version": "0.0.22",
  "type": "module",
  "main": "./dist/vite-ui-kit.umd.cjs",
  "module": "./dist/vite-ui-kit.js",
  "types": "./dist/src/index.d.ts",
  "description": "This is a simple utility package",
  "author": "Proxa-prog",
  "license": "MIT",
  "homepage": "https://github.com/Proxa-prog",
  "repository": {
    "type": "git",
    "url": "https://github.com/Proxa-prog/vite-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Proxa-prog/vite-ui-kit/issues"
  },
  "keywords": [
    "some",
    "keywords",
    "to",
    "describe",
    "the",
    "package"
  ],
  "exports": {
    ".": {
      "import": "./dist/vite-ui-kit.js",
      "require": "./dist/vite-ui-kit.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "puppeteer": "^19.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-storyshots-puppeteer": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/builder-vite": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@wdio/appium-service": "^8.9.0",
    "@wdio/cli": "^8.9.0",
    "@wdio/local-runner": "^8.9.0",
    "@wdio/mocha-framework": "^8.8.7",
    "@wdio/spec-reporter": "^8.8.7",
    "appium": "^1.22.3",
    "chromedriver": "^112.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest-image-snapshot": "^6.1.0",
    "jsdom": "^21.1.1",
    "playwright": "^1.33.0",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-test-utils": "^0.6.0",
    "vitest": "^0.30.1",
    "wdio-chromedriver-service": "^8.1.1"
  },
  "files": [
    "dist"
  ],
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
